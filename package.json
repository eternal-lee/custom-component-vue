{
  "name": "custom-component-vue",
  "version": "3.0.2",
  "private": false,
  "description": "自定义vue组件",
  "main": "lib/custom-component-vue.umd.min.js",
  "license": "ISC",
  "keywords": [
    "custom-package-vue",
    "vue",
    "custom-package",
    "buoyPopup"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:rechard-work/custom-component-vue.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:dev": "vue-cli-service build --mode dev",
    "build:test": "vue-cli-service build --mode test",
    "build:prod": "vue-cli-service build --mode prod",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name custom-component-vue --dest lib packages/index.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "style-resources-loader": "^1.4.1",
    "vant": "^3.0.18",
    "vue": "^3.0.0",
    "vue-axios": "^3.2.4",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^4.1.1",
    "less-loader": "^7.0.0",
    "prettier": "^2.3.1",
    "style-loader": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
